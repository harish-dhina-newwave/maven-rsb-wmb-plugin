<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.rsb.broker</groupId>
  <artifactId>maven-rsb-wmb-deploybar</artifactId>
  <packaging>maven-plugin</packaging>
  <version>1.0.0</version>
  <name>maven-rsb-wmb-deploybar plugin</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-plugin-api</artifactId>
      <version>2.0</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.ibm.mq</groupId>
      <artifactId>WebsphereMQClassesForJava</artifactId>
      <version>6.0.2.2</version>
    </dependency>
    <dependency>
      <groupId>javax.resource.cci</groupId>
      <artifactId>SunConnectorClasses</artifactId>
      <version>1.3.0 </version>
    </dependency>
    <dependency>
      <groupId>com.ibm.broker.config</groupId>
      <artifactId>WMBConfigManagerProxy</artifactId>
      <version>1.5.0</version>
    </dependency>
  </dependencies>
  <build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
